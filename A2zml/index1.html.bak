    <style>
        /* 统一盒模型 */
        * {
           -webkit-box-sizing: border-box;
            box-sizing: border-box;
        }

        /* 优化container类 */
       .container {
            margin: 0 auto;
            padding: 0 8px;
            max-width: 1280px;
        }

        @media (min-width: 600px) {
          .container {
                width: 94%;
            }
        }

        @media (min-width: 1024px) {
          .container {
                width: 92%;
            }
        }

        /* row类 */
       .row {
            margin: 0 -8px;
        }

       .row::after {
            display: table;
            clear: both;
            content: '';
        }

        /* col类 */
      .col {
            position: relative;
            min-height: 1px;
            padding: 0 8px;
        }

       .row.col {
            float: left;
            width: 50%;
        }

        @media (min-width: 1440px) {
          .row.col {
